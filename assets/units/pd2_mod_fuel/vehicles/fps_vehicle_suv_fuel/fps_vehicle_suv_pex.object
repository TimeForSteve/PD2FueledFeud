<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_dlc_pex/vehicles/fps_vehicle_suv_pex/fps_vehicle_suv_pex" orientation_object="rp_fps_vehicle_suv_pex" />
	<sequence_manager file="units/pd2_dlc_pex/vehicles/fps_vehicle_suv_pex/fps_vehicle_suv_pex" />

	<animation_def name="anims/units/pex_suv_and_heli/pex_suv_and_heli" />

	<animations>
		<animation_group name="anim_door" enabled="false">
				<object name="anim_door" />
		</animation_group>
		<animation_group name="anim_trunk" enabled="false">
				<object name="anim_hinges" />
				<object name="anim_trunk" />
		</animation_group>
		<animation_group name="ag_speedometer" enabled="false">
				<object name="a_speedometer" />
		</animation_group>
		<animation_group name="ag_rpm_meter" enabled="false">
				<object name="a_rpm_meter" />
		</animation_group>
		<animation_group name="anim_driver" enabled="false">
				<object name="Hips" />
				<object name="Spine" />
				<object name="Spine1" />
				<object name="Spine2" />
				<object name="Head" />
				<object name="LeftShoulder" />
				<object name="LeftArm" />
				<object name="LeftForeArm" />
				<object name="LeftHand" />
				<object name="RightShoulder" />
				<object name="RightArm" />
				<object name="RightForeArm" />
				<object name="RightHand" />
				<object name="LeftUpLeg" />
				<object name="LeftLeg" />
				<object name="LeftFoot" />
				<object name="RightUpLeg" />
				<object name="RightLeg" />
				<object name="RightFoot" />
		</animation_group>
	</animations>

	<bodies>
		<body name="body_chassis" enabled="false" template="vehicle" collision_script_filter="1" collision_script_quiet_time="0.5" collides_with_mover="false" collision_script_tag="vehicle_collision" give_standing_velocity="false" >
			<object name="rp_fps_vehicle_suv_pex" />
			<object name="c_box" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_vehicle" enabled="true" template="animated" >
			<object name="anim_body" />
			<object name="c_mesh_mopp" collision_type="mesh_mopp" padding="-2.5"/>
		</body>
		<body name="body_trunk" enabled="true" template="animated" >
			<object name="anim_trunk" />
			<object name="c_box_trunk" collision_type="box" padding="-2.5"/>
		</body>
		<body name="body_heli" enabled="true" template="animated" >
			<object name="a_body" />
			<object name="c_box_heli" collision_type="box" />
		</body>
	</bodies>

	 <effects>
		<effect_spawner name="es_smoke_med" enabled="false" object="snd_engine" effect="effects/payday2/particles/vehicle/vehicle_damage_small_smoke" />
		<effect_spawner name="es_smoke_full" enabled="false" object="snd_engine" effect="effects/payday2/particles/vehicle/vehicle_damage_big_smoke" />
		<effect_spawner name="es_heli" enabled="false" object="rp_fps_vehicle_suv_pex" effect="effects/particles/dest/bridge_heli_smoke" />
		<effect_spawner name="car_exhaust_01" enabled="false" object="e_effect_01" effect="effects/payday2/particles/smoke_trail/smoke_trail_wanker_car_no_windup" />
		<effect_spawner name="car_exhaust_02" enabled="false" object="e_effect_02" effect="effects/payday2/particles/smoke_trail/smoke_trail_wanker_car_no_windup" />
	</effects>

	<decal_surfaces default_material="metal">
		<decal_mesh name="dm_vehicle" 				enabled="true" material="metal_chassis" />
	</decal_surfaces>

	<graphics>
		<object name="g_lootbag_1" 		enabled="false" />
		<object name="g_lootbag_2" 		enabled="false" />
		<object name="g_lootbag_3" 		enabled="false" />
		<object name="g_lootbag_4" 		enabled="false" />
		<object name="g_lootbag_5" 		enabled="false" />
		<object name="g_lootbag_6" 		enabled="false" />
		<graphic_group name="grp_vehicle" enabled="true" culling_object="c_culling">
			<lod_object name="lod_vehicle_body">
				<object name="g_vehicle_body" enabled="true">
					<material materials="mtr_vehicle" max_distance="2500"/>
					<material materials="mtr_vehicle_lod"/>
				</object>
			</lod_object>
			<lod_object name="lod_vehicle_door">
				<object name="g_door_driver" enabled="true">
					<material materials="mtr_vehicle" max_distance="2500"/>
					<material materials="mtr_vehicle_lod"/>
				</object>
			</lod_object>
			<lod_object name="lod_vehicle_trunk">
				<object name="g_trunk" enabled="true">
					<material materials="mtr_vehicle" max_distance="2500"/>
					<material materials="mtr_vehicle_lod"/>
				</object>
			</lod_object>
			<lod_object name="lod_vehicle_stickers">
				<object name="g_stickers" enabled="true">
					<material materials="mtr_vehicle" max_distance="2500"/>
					<material materials="mtr_vehicle_lod"/>
				</object>
			</lod_object>
			<lod_object name="lod_vehicle_grill">
				<object name="g_grill" enabled="true">
					<material materials="mat_bex_suv_grill" max_distance="2500"/>
					<material materials="mat_bex_suv_grill_lod"/>
				</object>
			</lod_object>
			<lod_object name="lod_vehicle_armor">
				<object name="g_armor" enabled="true">
					<material materials="mat_bex_suv_armor" max_distance="2500"/>
					<material materials="mat_bex_suv_armor_lod"/>
				</object>
			</lod_object>
			<lod_object name="lod_tire_front_left">
				<object name="g_tire_front_left" enabled="true" max_distance="2500" />
				<object name="g_tire_front_left_lod1" enabled="true"/>
			</lod_object>
			<lod_object name="lod_tire_front_right">
				<object name="g_tire_front_right" enabled="true" max_distance="2500" />
				<object name="g_tire_front_right_lod1" enabled="true"/>
			</lod_object>
			<lod_object name="lod_tire_rear_left">
				<object name="g_tire_rear_left" enabled="true" max_distance="2500" />
				<object name="g_tire_rear_left_lod1" enabled="true"/>
			</lod_object>
			<lod_object name="lod_tire_rear_right">
				<object name="g_tire_rear_right" enabled="true" max_distance="2500" />
				<object name="g_tire_rear_right_lod1" enabled="true"/>
			</lod_object>
			<object name="g_mullplan" 				enabled="true"/>
			<object name="g_glass" 					enabled="true" shadow_caster="false" />
			<object name="g_glass_right" 			enabled="true" shadow_caster="false" />
			<object name="g_glass_interior" 		enabled="true" shadow_caster="false" />
			<object name="g_glass_trunk" 			enabled="true" shadow_caster="false" />
			<object name="g_interior" 				enabled="true"/>
			<object name="g_trunk_llining" 			enabled="true"/>
			<object name="g_hinges" 				enabled="true"/>
			<object name="g_pistons" 				enabled="true"/>
			<object name="s_vehicle" 				enabled="true" shadow_caster="true" />
			<object name="s_wheels" 				enabled="true" shadow_caster="true" />
			<object name="g_door_glass" 			enabled="true"/>
			<object name="g_armor_door" 			enabled="true"/>
			<object name="g_interior_door" 			enabled="true"/>
		</graphic_group>

		<graphic_group name="grp_icon_entry" enabled="true">
			<lod_object name="lod_icon_drive">
				<object name="g_icon_door_drive_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_door_drive_lod1" enabled="true" />
			</lod_object>
			<lod_object name="lod_icon_front_r_pass">
				<object name="g_icon_door_front_r_pass_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_door_front_r_pass_lod1" enabled="true" />
			</lod_object>
			<lod_object name="lod_icon_rear_l_pass">
				<object name="g_icon_door_rear_l_pass_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_door_rear_l_pass_lod1" enabled="true" />
			</lod_object>
			<lod_object name="lod_icon_rear_r_pass">
				<object name="g_icon_door_rear_r_pass_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_door_rear_r_pass_lod1" enabled="true" />
			</lod_object>
		</graphic_group>

		<graphic_group name="grp_heli" enabled="true" culling_object="c_culling_heli">
			<object name="g_interior_heli" enabled="true"/>
			<object name="g_door_right_swat" enabled="true"/>
			<object name="g_door_left_swat" enabled="true"/>
			<object name="g_main_rotor" enabled="true" />
			<object name="g_second_rotor" enabled="true"/>
			<object name="g_body_swat" enabled="true"/>
			<object name="g_glass_heli" enabled="true"/>
			<object name="g_wire_attachment" enabled="true" shadow_caster="true"/>
			<object name="g_wire" enabled="true" shadow_caster="true"/>
			<object name="s_heli" enabled="true" shadow_caster="true"/>
			<object name="s_rotor" enabled="true" shadow_caster="true"/>
			<object name="s_second_rotor" enabled="true" shadow_caster="true"/>
		</graphic_group>

		<graphic_group name="grp_prisoner" enabled="false" culling_object="co_prisoner_normal">
			<object name="g_body_prisoner" enabled="true" />
			<object name="g_hair_prisoner" enabled="true" />
		</graphic_group>
		<graphic_group name="grp_head" enabled="false" culling_object="co_prisoner">
			<object name="g_head" 			enabled="true" />
			<object name="g_acc_headset" 	enabled="true" />
		</graphic_group>
		<graphic_group name="grp_prisoner_disguise" enabled="false" culling_object="co_prisoner_cop">
			<object name="g_body_police" enabled="true" />
			<object name="g_arms_police" enabled="true" />
			<object name="g_hair_police" enabled="true" />
		</graphic_group>
		<graphic_group name="grp_icon_repair" enabled="true">
			<lod_object name="lod_icon_repair">
				<object name="g_icon_repair_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_repair_lod1" enabled="true" />
			</lod_object>
		</graphic_group>
	</graphics>


	<vehicle name="vhc_vehicle">
		<!-- IMPORTANT: suspension supported_mass must be 1/4 of chassis mass, if not vehicle behavior is undefined -->
		<chassis body="body_chassis" mass="3000" center_of_mass_offset="0.0 0.0 0.1" inertia_multiplier="1.0 1.0 1.0" downforce_strength="1.0" drag_strength="1.0" />
		<wheels>
			<wheel spin_object="anim_tire_front_left"	turn_object=""	radius="0.1" width="0.5" mass="3000" transmission_ratio="1.0" is_steering="false" max_steering="0.01" braking_torque="10000" handbrake_torque="0" />
			<wheel spin_object="anim_tire_front_right"	turn_object=""	radius="0.1" width="0.5" mass="3000" transmission_ratio="1.0" is_steering="false" max_steering="0.01" braking_torque="10000" handbrake_torque="0" />
			<wheel spin_object="anim_tire_rear_left"	turn_object=""	radius="0.1" width="0.5" mass="3000" transmission_ratio="1.0" is_steering="false" max_steering="0.01" braking_torque="10000" handbrake_torque="0" />
			<wheel spin_object="anim_tire_rear_right"	turn_object=""	radius="0.1" width="0.5" mass="3000" transmission_ratio="1.0" is_steering="false" max_steering="0.01" braking_torque="10000" handbrake_torque="0" />
		</wheels>
		<suspension max_compression="0.18" max_extension="0.22" spring_strength="55000" damper_rate="9500" supported_mass="1000" />
		<gearbox switch_time="0.1" ratio_multiplier="4.0">
			<ratios>
				<ratio value="-3.0" />
				<ratio value="0.0" />
				<ratio value="4.0" />
				<ratio value="2.0" />
				<ratio value="1.6" />
				<ratio value="1.2" />
			</ratios>
		</gearbox>
		<engine max_torque="1" max_rpm="4500"/>
	</vehicle>

</dynamic_object>