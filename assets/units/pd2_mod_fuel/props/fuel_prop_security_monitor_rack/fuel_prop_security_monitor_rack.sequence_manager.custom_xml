		</sequence>
		<sequence editable_state="true" name="'state_set_top_time_08'" triggable="true">
			<run_sequence name="'hide_top_times'"/>
			<object enabled="true" name="'g_time_top_8'"/>
		</sequence>
		<sequence editable_state="true" name="'state_set_bottom_time_01'" triggable="true">
