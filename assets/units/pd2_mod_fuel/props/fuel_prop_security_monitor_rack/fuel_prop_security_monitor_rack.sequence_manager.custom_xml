		</sequence>
		<sequence editable_state="true" name=" 'state_screen_on' " triggable="true">
			<object enabled="true" name="'g_screen_on'"/>
			<object enabled="false" name="'g_screen_off'"/>
		</sequence>
		<sequence editable_state="true" name=" 'state_screen_off' " triggable="true">
			<run_sequence name="'hide_all_names'"/>
			<run_sequence name="'hide_all_times'"/>
			<object enabled="false" name="'g_screen_on'"/>
			<object enabled="true" name="'g_screen_off'"/>
		</sequence>
		<sequence editable_state="true" name="'state_interaction_enable'" triggable="true">
			<interaction enabled="true"/>
		</sequence>
		<sequence editable_state="true" name="'state_interaction_disable'" triggable="true">
			<interaction enabled="false"/>
		</sequence>
		<sequence editable_state="false" name="'interact'" triggable="false">
			<run_sequence name="'channel_swiched'"/>
		</sequence>
		<sequence editable_state="true" name="'channel_swiched'" triggable="true">
			<sound action="'play'" event="'tv_switch_on'" source="'snd'"/>
			<sound action="'play'" event="'tv_switch_off'" source="'snd'" start_time="15/30"/>
		</sequence>
	</unit>
</table>
