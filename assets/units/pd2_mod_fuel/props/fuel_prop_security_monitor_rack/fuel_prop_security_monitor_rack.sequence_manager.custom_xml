		</sequence>
		<sequence editable_state="true" name="'state_set_name_06'" triggable="true">
			<run_sequence name="'hide_bottom_names'"/>
			<object enabled="true" name="'g_name_6'"/>
		</sequence>
		<sequence editable_state="true" name="'state_set_name_07'" triggable="true">
			<run_sequence name="'hide_bottom_names'"/>
			<object enabled="true" name="'g_name_7'"/>
		</sequence>
		<sequence editable_state="true" name="'state_set_name_08'" triggable="true">
			<run_sequence name="'hide_bottom_names'"/>
			<object enabled="true" name="'g_name_8'"/>
		</sequence>
		<sequence editable_state="true" name="'state_set_top_time_01'" triggable="true">
			<run_sequence name="'hide_top_times'"/>
			<object enabled="true" name="'g_time_top_1'"/>
		</sequence>
		<sequence editable_state="true" name="'state_set_top_time_02'" triggable="true">
			<run_sequence name="'hide_top_times'"/>
			<object enabled="true" name="'g_time_top_2'"/>
		</sequence>
		<sequence editable_state="true" name="'state_set_top_time_03'" triggable="true">
			<run_sequence name="'hide_top_times'"/>
			<object enabled="true" name="'g_time_top_3'"/>
		</sequence>
		<sequence editable_state="true" name="'state_set_top_time_04'" triggable="true">
