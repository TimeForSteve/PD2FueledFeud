<table>
	<unit>
		<sequence name="'hide_all_names'">
			<run_sequence name="'hide_top_names'"/>
			<run_sequence name="'hide_bottom_names'"/>
		</sequence>
		<sequence name="'hide_top_names'">
			<object enabled="false" name="'g_name_1'"/>
			<object enabled="false" name="'g_name_2'"/>
			<object enabled="false" name="'g_name_3'"/>
			<object enabled="false" name="'g_name_4'"/>
		</sequence>
		<sequence name="'hide_bottom_names'">
			<object enabled="false" name="'g_name_5'"/>
			<object enabled="false" name="'g_name_6'"/>
			<object enabled="false" name="'g_name_7'"/>
			<object enabled="false" name="'g_name_8'"/>
		</sequence>
		<sequence name="'hide_all_times'">
			<run_sequence name="'hide_top_times'"/>
			<run_sequence name="'hide_bottom_times'"/>
		</sequence>
		<sequence name="'hide_top_times'">
			<object enabled="false" name="'g_time_top_1'"/>
			<object enabled="false" name="'g_time_top_2'"/>
			<object enabled="false" name="'g_time_top_3'"/>
			<object enabled="false" name="'g_time_top_4'"/>
			<object enabled="false" name="'g_time_top_5'"/>
			<object enabled="false" name="'g_time_top_6'"/>
			<object enabled="false" name="'g_time_top_7'"/>
			<object enabled="false" name="'g_time_top_8'"/>
		</sequence>
		<sequence name="'hide_bottom_times'">
			<object enabled="false" name="'g_time_bottom_1'"/>
			<object enabled="false" name="'g_time_bottom_2'"/>
			<object enabled="false" name="'g_time_bottom_3'"/>
			<object enabled="false" name="'g_time_bottom_4'"/>
			<object enabled="false" name="'g_time_bottom_5'"/>
			<object enabled="false" name="'g_time_bottom_6'"/>
			<object enabled="false" name="'g_time_bottom_7'"/>
		</sequence>
		<sequence editable_state="true" name="'state_set_name_01'" triggable="true">
			<run_sequence name="'hide_top_names'"/>
			<object enabled="true" name="'g_name_1'"/>
		</sequence>
		<sequence editable_state="true" name="'state_set_name_02'" triggable="true">
