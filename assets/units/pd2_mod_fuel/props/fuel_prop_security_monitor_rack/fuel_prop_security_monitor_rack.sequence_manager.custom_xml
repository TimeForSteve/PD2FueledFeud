		</sequence>
		<sequence editable_state="true" name="'state_set_bottom_time_03'" triggable="true">
			<run_sequence name="'hide_bottom_times'"/>
			<object enabled="true" name="'g_time_bottom_3'"/>
		</sequence>
		<sequence editable_state="true" name="'state_set_bottom_time_04'" triggable="true">
			<run_sequence name="'hide_bottom_times'"/>
