<dynamic_object>
	<diesel materials="units/pd2_mod_custom/cus_loot_m4/cases/cus_loot_m4_case" orientation_object="rp_gen_interactable_weapon_case_2x1" />
	<sequence_manager file="units/pd2_mod_custom/cus_loot_m4/cases/cus_loot_m4_case_z_axis" />
	<animations>
		<animation_group name="anim" enabled="false">
			<object name="jt_box" />
			<object name="anim_hinge" />
			<object name="anim_lock" />
		</animation_group>
	</animations>
	<bodies>
		<body name="body_closed" enabled="true" template="static">
			<object name="jt_box" />
			<object name="g_weapon_case" collision_type="box" />
		</body>
		<body name="body_open" enabled="false" template="static">
			<object name="rp_gen_interactable_weapon_case_2x1" />
			<object name="c_medium_open" collision_type="mesh_mopp" />
		</body>
	</bodies>
	<decal_surfaces default_material="plastic">
		<decal_mesh name="g_weapon_case" 	enabled="true" material="plastic" />
		<decal_mesh name="dm_medium_open" 	enabled="false" material="plastic" />
		<decal_mesh name="dm_foam" 			enabled="false" material="cloth_stuffed" />
	</decal_surfaces>

	<graphics>
		<graphic_group name="grp_wpn" enabled="true" culling_object="co_cull">
			<object name="g_weapon_case" enabled="true" />
		</graphic_group>
	</graphics>
</dynamic_object>